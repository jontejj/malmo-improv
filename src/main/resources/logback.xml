<configuration>
	<contextListener class="ch.qos.logback.classic.jul.LevelChangePropagator">
	<resetJUL>true</resetJUL>
	</contextListener>	
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg
			</pattern>
		</encoder>
	</appender>

	<logger name="com.malmoimprov" level="WARN" />
	<!-- <logger name="com.google.appengine.tools.development.DevAppServerModulesFilter" level="INFO" /> -->
	<!--<logger name="org.atmosphere.util.IOUtils" level="INFO" /> -->
	<root level="WARN">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>